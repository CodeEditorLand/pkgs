{
	"name": "@codeeditorland/extra-packages-workspace",
	"funding": {
		"type": "opencollective",
		"url": "HTTPS://opencollective.com/swc"
	},
	"lint-staged": {
		"!(**/tests/**/*)*.js": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.jsx": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.ts": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.tsx": [
			"prettier --write"
		],
		"**": [
			"prettier --write --ignore-unknown"
		],
		"*.rs": [
			"rustfmt --"
		],
		"*.toml": [
			"taplo format"
		]
	},
	"dependencies": {},
	"devDependencies": {
		"@changesets/cli": "2.27.11",
		"@taplo/cli": "0.7.0",
		"cross-env": "7.0.3",
		"cspell": "8.17.2",
		"lerna": "8.1.9",
		"lint-staged": "15.4.1"
	}
}
